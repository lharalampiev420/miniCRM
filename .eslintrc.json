{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["node"],
  "rules": {
    "node/prefer-global/console": ["error", "always"],
    "no-console": "warn",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "req|req|next|" }],
    "prefer-destructuring": "warn",
    "no-duplicate-imports": ["error", { "includeExports": true }]
  }
}
